{
  "name": "mini-task-scheduler-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 5174 --strictPort",
    "build": "tsc -b && vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview --host",
    "test": "vitest run",
    "test:ui": "vitest --config vitest.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:trace": "playwright test --trace on",
    "check-types": "tsc --noEmit",
    "test:watch": "vitest"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/expect": "^2.1.9",
    "@vitest/mocker": "^2.1.9",
    "@vitest/pretty-format": "^2.1.9",
    "@vitest/runner": "^2.1.9",
    "@vitest/snapshot": "^2.1.9",
    "@vitest/utils": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "cac": "^6.7.14",
    "chai": "^5.3.3",
    "eslint": "^8.57.0",
    "jsdom": "^27.0.1",
    "pathe": "^1.1.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "tinypool": "^1.1.1",
    "tinyrainbow": "^1.2.0",
    "tinyspy": "^3.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.4.21",
    "vite-node": "^2.1.9",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
